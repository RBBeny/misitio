<!DOCTYPE html>
<html lang="es">

<head>
  <%- include('../partials/head'); %>
</head>

<body>
  <%- include('../partials/navbar'); %>

  <div class="jumbotron text-aling">
    <h1 class="display-4">Videos 

    </h1>
    <p class="lead">Aqui muestro videos sobre proyectos que he realizado</p>
    
    <hr class="my-4">

    
    <div id="player"></div>

    <hr class="my-4">


    <script>
      var tag = document.createElement('script');

      tag.src = "https://www.youtube.com/iframe_api";
      var firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);


      var player;
      function onYouTubeIframeAPIReady() {
        player = new YT.Player('player', {
          height: '360',
          width: '640',
          videoId: 'SV8NFEwGnFQ',
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        });
      }
      function onPlayerReady(event) {
        event.target.playVideo();
      }

      var done = false;
      function onPlayerStateChange(event) {
        if (event.data == YT.PlayerState.PLAYING && !done) {
          setTimeout(stopVideo, 6000);
          done = true;
        }
      }
      function stopVideo() {
        player.stopVideo();
      }
    </script>
    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous"
      src="https://connect.facebook.net/es_ES/sdk.js#xfbml=1&version=v8.0&appId=2781826215438098&autoLogAppEvents=1"
      nonce="eRJa1t5v"></script>
    <div class="fb-comments" data-href="https://developers.facebook.com/docs/plugins/comments#configurator"
      data-numposts="5" data-width=""></div>

    
  </div>







  <%- include('../partials/footer'); %>
  <%- include('../partials/script'); %>
</body>

</html>